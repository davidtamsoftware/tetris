{
  "name": "tetris-core",
  "version": "0.0.0",
  "description": "Tetris game logic and domain models",
  "author": "David Tam <david.tam.kc@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/tetris-core.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "tslint": "^5.11.0"
  },
  "dependencies": {
    "core-js": "^2.5.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/src/.*(/__tests__/|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
